{
    "name": "engage-php/luminary",
    "description": "An api service based on laravel lumen",
    "require": {
        "laravel/lumen": "5.4.*",
        "wikimedia/composer-merge-plugin": "^1.4",
        "league/flysystem-aws-s3-v3": "~1.0",
        "eddturtle/direct-upload": "^1.6",
        "illuminate/redis": "5.4.*",
        "barryvdh/laravel-cors": "^0.9.0@dev",
        "symfony/var-dumper": "^4.0@dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "symfony/routing": "^3.0"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "api/composer.json"
            ],
            "recurse": true,
            "replace": true,
            "ignore-duplicates": true,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "autoload": {
        "psr-4": {
            "Api\\": "api/",
            "Luminary\\": "luminary/"
        },
        "files": [
            "luminary/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Matthew Cuyar",
            "email": "matt@enctypeapparel.com"
        }
    ],
    "minimum-stability" : "dev"
}
