{
    "name": "engage-php/luminary",
    "description": "An api service based on laravel lumen",
    "require": {
        "laravel/lumen": "^5.5",
        "wikimedia/composer-merge-plugin": "^1.4",
        "league/flysystem-aws-s3-v3": "~1.0",
        "eddturtle/direct-upload": "^1.6",
        "illuminate/redis": "5.5.*",
        "barryvdh/laravel-cors": "^0.9.0@dev",
        "symfony/var-dumper": "^4.0@dev",
        "spatie/laravel-permission": "~2.0",
        "tymon/jwt-auth": "1.0.*@dev",
        "nicolaslopezj/searchable": "1.*",
        "doctrine/dbal": "^2.6",
        "fzaninotto/faker": "~1.4",
        "symfony/http-kernel": "~3.4",
        "symfony/http-foundation": "~3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "symfony/routing": "^3.0",
        "filp/whoops": "~2.0",
        "pda/pheanstalk": "^3.1"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "api/composer.json"
            ],
            "recurse": true,
            "replace": true,
            "ignore-duplicates": true,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "autoload": {
        "psr-4": {
            "Api\\": "api/",
            "Luminary\\": "luminary/"
        },
        "files": [
            "luminary/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Matthew Cuyar",
            "email": "matt@enctypeapparel.com"
        }
    ],
    "minimum-stability" : "stable"
}
